{
  "name": "ai-root",
  "version": "0.0.1",
  "description": "Tsailab library",
  "author": "tsailab",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"**/*.json\" \"packages/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/ai-root/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/ai-root/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/config": "3.2.3",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/passport": "10.0.3",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/swagger": "8.1.0",
    "@nestjs/testing": "^10.0.0",
    "@nestjs/typeorm": "10.0.2",
    "@tsailab/core-types": "0.2.0",
    "@tsailab/schematics": "^1.2.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^22.0.0",
    "@types/passport-jwt": "4.0.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@xtsai/xai-utils": "0.0.8",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "iterare": "1.2.1",
    "jest": "^29.5.0",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "0.3.20",
    "typescript": "~5.4.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/packages/"
    ],
    "moduleNameMapper": {
      "^@xtsai/core(|/.*)$": "<rootDir>/packages/core/dist/$1",
      "^@xtsai/system(|/.*)$": "<rootDir>/packages/system/dist/$1"
    }
  }
}
