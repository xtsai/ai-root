{
  "name": "@xtsai/core",
  "version": "0.0.1",
  "description": "Tsailab library core",
  "author": "tsailab",
  "license": "LICENSE",
  "keywords": [
    "tsai-platform",
    "core"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "pnpm clean && pnpm build:pkg",
    "build:pkg": "tsc --build --verbose --force tsconfig.pkg.json",
    "clean": "rm -rf dist/*",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,packages,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@nestjs/config": "3.2.3",
    "@tsailab/ioredis-mq": "0.0.7",
    "ioredis": "5.4.1",
    "iterate": "0.1.1",
    "ms": "3.0.0-canary.1",
    "request-ip": "3.3.0",
    "svg-captcha-fixed": "1.5.2",
    "uid": "2.0.2"
  },
  "devDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "tslib": "2.8.1",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.4.3 || ^10.0.0",
    "@nestjs/testing": "^9.4.3 || ^10.0.0",
    "tslib": "2.8.1",
    "typescript": "^4.1.6 || ^5.1.3"
  },
  "peerDependenciesMeta": {
    "uid": {
      "optional": true
    },
    "@tsailab/ioredis-mq": {
      "optional": false
    }
  }
}
